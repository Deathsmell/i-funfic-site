{
  "name": "i-funfic-site",
  "version": "1.0.0",
  "main": "app.js",
  "author": "Alexander Mitskevich <johndeathsmell@gmail.com>",
  "license": "MIT",
  "engines": {
    "yarn": ">=1.22.5"
  },
  "scripts": {
    "start": "NODE_ENV=production node app.js",
    "server:testprod": "yarn client:build && NODE_ENV=production ts-node app.ts",
    "server:start": "NODE_ENV=production ts-node app.ts",
    "server:dev": "nodemon app.ts",
    "client:dev": "yarn --cwd ./client start",
    "client:build": "yarn --cwd ./client build",
    "dev": "concurrently \"yarn server:dev\" \"yarn client:dev\"",
    "heroku": "yarn client:build && git add . && git commit -m \"Heroku deploy\" && git push heroku deploy:master"
  },
  "dependencies": {
    "@stomp/stompjs": "^6.0.0",
    "@tsconfig/node12": "^1.0.7",
    "@types/cors": "^2.8.8",
    "axios": "^0.21.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.23.0",
    "cors": "^2.8.5",
    "datauri": "^3.0.0",
    "draftjs-to-markdown": "^0.6.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.0",
    "express-session": "^1.17.1",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.4.16",
    "passport": "^0.4.1",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.11.0",
    "pg": "^8.4.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5",
    "websocket": "^1.0.32",
    "ws": "^7.4.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.8",
    "@types/express-fileupload": "^1.1.5",
    "@types/express-session": "^1.17.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.5",
    "@types/nodemailer": "^6.4.0",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/pdfkit": "^0.10.6",
    "@types/pg": "^7.14.5",
    "@types/sequelize": "^4.28.9",
    "@types/validator": "^13.1.0",
    "@types/websocket": "^1.0.1",
    "@types/ws": "^7.4.0",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
